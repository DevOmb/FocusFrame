<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FocusFrame">
        <attr name="focusBox_height" format="dimension" />
        <attr name="focusBox_width" format="dimension" />
        <attr name="focusBox_position" format="enum">
            <enum name="top_left" value="0" />
            <enum name="top_center" value="1" />
            <enum name="top_right" value="2" />
            <enum name="center_left" value="3" />
            <enum name="center_center" value="4" />
            <enum name="center_right" value="5" />
            <enum name="bottom_left" value="6" />
            <enum name="bottom_center" value="7" />
            <enum name="bottom_right" value="8" />
        </attr>
        <attr name="focusBox_shape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="square" value="1" />
            <enum name="circle" value="2" />
        </attr>
        <attr name="movable" format="boolean" />
        <attr name="focusBox_borderWidth" format="dimension" />
        <attr name="focusBox_borderColor" format="color" />
        <attr name="focusBox_movingBorderColor" format="color" />
    </declare-styleable>

</resources>